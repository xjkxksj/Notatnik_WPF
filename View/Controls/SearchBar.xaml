<UserControl x:Class="Notatnik_WPF.SearchBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Notatnik_WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Border CornerRadius="20,20,20,20" BorderBrush="Black" Background="Snow" MaxHeight="60" MinHeight="40">

        <StackPanel VerticalAlignment="Center">
            <TextBox Margin="20" FontSize="28"></TextBox>

        </StackPanel>
    </Border>

</UserControl>
